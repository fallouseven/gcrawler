                Mot clef :
            </div>
            <div class="col-md-10">
                @Html.TextBox("keyWords")
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                Mot clef :
            </div>
            <div class="col-md-10">
                @Html.TextBox("keyWords")
            </div>
        </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

@model IEnumerable<gCrawler.Models.Project>
<div class="container body-content">
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>Name</th>
            <th>Options</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdProject)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.IdProject }) |
                    @Html.ActionLink("Details", "Details", new { id = item.IdProject }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.IdProject })|
                    @Html.ActionLink("Résultats du crawl", "Occurrence", new { id = item.IdProject })
                </td>
            </tr>
        }

    </table>
</div>